<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.netbull.shop.area.dao.AreaDao">

   
   <select id="findAll" resultType="com.netbull.shop.area.entity.Area" >
	    SELECT  areaID,areaName,parentID ,C.index FROM tb_medical_area C order by C.index
	</select>
   
    <select id="query_by_params" parameterType="java.util.Map" resultType="com.netbull.shop.area.entity.Area">
       SELECT  areaID,areaName,parentID ,C.index FROM tb_medical_area C WHERE C.parentID=#{parentID} order by C.index asc
   </select>
   
    <select id="queryEntityById" parameterType="java.lang.Integer" resultType="com.netbull.shop.area.entity.Area">
        SELECT * FROM tb_medical_area WHERE areaID=#{areaID}
    </select>
</mapper>