<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.netbull.shop.disease.entity.Disease">

  <select id="query_by_sectionID" parameterType="java.lang.Long" resultType="com.netbull.shop.disease.entity.Disease">
      select d.* from tb_medical_disease d where d.sectionID = #{sectionID}
  </select>
	
</mapper>