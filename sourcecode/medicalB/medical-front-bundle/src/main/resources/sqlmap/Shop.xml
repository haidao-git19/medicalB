<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.netbull.web.index.dao.ShopDao">

   <select id="countShop" parameterType="map"
  	 resultType="long">
        SELECT COUNT(1)  FROM tb_medical_shop_bak C
        where 1=1
         and C.state=0 
   </select>
   
</mapper>